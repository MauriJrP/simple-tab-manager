{
	"name": "Simple Tab Manager",
	"description": "A simple tabs manager extension",
	"version": "0.1.0",
	"manifest_version": 3,
	"background": {
		"service_worker": "background.js"
	},
	"permissions": ["storage", "tabs", "tabGroups"],
	"action": {
		"default_popup": "index.html"
	},
	"options_page": "options/options.html"
}
